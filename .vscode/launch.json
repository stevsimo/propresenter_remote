    "version": "0.1.0",
    "configurations": [
        {
            "name": "propresenter_remote",
            "request": "launch",
            "type": "dart",
            "args": [
                "--web-port",
                "58047",
                "--web-browser-flag",
                "--disable-web-security"
            ],
        },
        {
            "name": "propresenter_remote (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile",
            "args": [
                "--web-port",
                "58047",
                "--web-browser-flag",
                "--disable-web-security"
            ],
        },
        {
            "name": "propresenter_remote (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release",
            "args": [
                "--web-port",
                "58047",
                "--web-browser-flag",
                "--disable-web-security"
            ],
        }
    ]
}